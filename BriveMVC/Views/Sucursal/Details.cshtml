    </div>
    <hr />
    <div class="card-body">
        <h5 class="card-title">Cliente</h5>
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdSucursal)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.IdSucursal)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.NombreSucursal)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NombreSucursal)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Direccion)
